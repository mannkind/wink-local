{
  "name": "WinkLocalWebUI",
  "version": "0.5.0",
  "description": "A web UI for Wink Local Control",
  "author": "Dustin Brewer",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.1",
    "bootstrap": "^3.3.7",
    "react": "^15.4.2",
    "react-color": "^2.11.4",
    "react-dom": "^15.4.2",
    "react-hot-loader": "next",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "@types/jest": "^16.0.4",
    "@types/react": "^15.0.4",
    "@types/react-dom": "^0.14.21",
    "@types/react-test-renderer": "^15.4.2",
    "neutrino": "^5.0.0",
    "neutrino-preset-jest": "^5.0.0",
    "neutrino-preset-react": "^5.0.0",
    "neutrino-preset-typescript": "^2.0.0",
    "tslint": "^5.1.0",
    "tslint-react": "^2.5.0",
    "typescript": "^2.2.2"
  },
  "neutrino": {
    "options": {
      "output": "./dist",
      "html": {
        "title": "Wink-Local UI"
      }
    },
    "config": {
      "entry": {
        "vendor": [
          "react",
          "react-dom",
          "react-redux",
          "react-color",
          "redux",
          "axios"
        ]
      }
    },
    "use": [
      "neutrino-preset-jest",
      "neutrino-preset-react",
      "neutrino-preset-typescript"
    ]
  },
  "scripts": {
    "start": "./node_modules/neutrino/bin/neutrino start",
    "build": "./node_modules/neutrino/bin/neutrino build",
    "test": "./node_modules/neutrino/bin/neutrino test",
    "lint": "./node_modules/tslint/bin/tslint -c tslint.json --fix 'src/**/*.ts*'"
  }
}
