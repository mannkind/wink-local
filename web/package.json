{
  "name": "wink-local-ui",
  "version": "0.2.0",
  "description": "A simple control panel for wink-mqtt",
  "author": "Dustin Brewer",
  "repository": "github.com/mannkind/wink-mqtt",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "keywords": [
    "vue",
    "vueify",
    "typescript"
  ],
  "scripts": {
    "watchify": "mkdirp debug && watchify -vd -p tsify -p browserify-hmr -e src/index.ts -o debug/index.js",
    "serve": "http-server -c 1 -a localhost -o",
    "start": "npm-run-all --parallel watchify serve",
    "dist": "mkdirp dist && rplcr index.html -f debug/ -o dist/index.html && cross-env NODE_ENV=production browserify -g envify -p tsify -p [ vueify/plugins/extract-css -o dist/index.css ] -e src/index.ts > dist/index.js"
  },
  "devDependencies": {
    "@types/axios": "^0.9.34",
    "axios": "^0.15.3",
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-runtime": "^6.0.0",
    "browserify": "^13.1.0",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^3.1.2",
    "envify": "^4.0.0",
    "http-server": "^0.9.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^3.1.0",
    "rplcr": "^0.2.2",
    "tsify": "^2.0.2",
    "tslint": "^4.2.0",
    "typescript": "^2.1.4",
    "uglify-js": "^2.5.0",
    "vue": "^2.1.8",
    "vue-color": "^2.0.3",
    "vue-typescript-component": "^0.6.1",
    "vue-typescript-import-dts": "^2.0.0",
    "vueify": "^9.4.0",
    "watchify": "^3.4.0"
  },
  "browserify": {
    "transform": [
      "vueify"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
