check process wink-mqtt matching "wink-mqtt"
    group wink-services
    if total memory > 34 MB then restart
    if total cpu > 80% for 8 cycles then restart
    if 8 restarts within 40 cycles then exec "/bin/sh -c 'reboot'"
    start program = "/etc/rc.d/init.d/wink-mqtt start"
    stop program = "/etc/rc.d/init.d/wink-mqtt stop"
